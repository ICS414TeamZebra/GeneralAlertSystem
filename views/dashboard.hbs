<h2>{{title}}</h2>
<h3>Actions</h3>
<ul>
  <li><a href="/alert/live">Send a Live Alert</a></li>
  <li><a href="/alert/test">Send a Test Alert</a></li>
  <li><a href="/cancel">Send an Alert Cancellation</a></li>
  <li><a href="/log">See all system actions</a></li>
</ul>
<h3>Recent Alerts</h3>
<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Type</th>
      <th>Event</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    {{#each alerts}}
    <tr>
      <td>{{date}}</td>
      <td>{{type}}</td>
      <td>{{event}}</td>
      <td>{{status}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>